generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRESQL_URL")
}

model User {
  id             Int      @id @default(autoincrement())
  username       String   @unique
  realName       String
  email          String   @unique
  password       String
  profilePicture String?
  createdAt      DateTime @default(now())
}
